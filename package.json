{
  "name": "hwp",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/style.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/style.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jennthn/hwp.git"
  },
  "author": "Jennifer Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jennthn/hwp/issues"
  },
  "homepage": "https://github.com/jennthn/hwp#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "ejs": "^2.7.1",
    "ejs-lint": "^0.3.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "1.0.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
